<%- model_class = @bunker.class -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:municipio) %>:</strong></dt>
  <dd><%= @bunker.municipio %></dd>
  <dt><strong><%= model_class.human_attribute_name(:en_operacion) %>:</strong></dt>
  <dd><%= @bunker.en_operacion %></dd>
  <dt><strong><%= model_class.human_attribute_name(:en_ruta) %>:</strong></dt>
  <dd><%= @bunker.en_ruta %></dd>
  <dt><strong><%= model_class.human_attribute_name(:nombre) %>:</strong></dt>
  <dd><%= @bunker.nombre %></dd>
  <dt><strong><%= model_class.human_attribute_name(:casilla_uno) %>:</strong></dt>
  <dd><%= @bunker.casilla_uno %></dd>
  <dt><strong><%= model_class.human_attribute_name(:casilla_dos) %>:</strong></dt>
  <dd><%= @bunker.casilla_dos %></dd>
  <dt><strong><%= model_class.human_attribute_name(:casilla_tres) %>:</strong></dt>
  <dd><%= @bunker.casilla_tres %></dd>
  <dt><strong><%= model_class.human_attribute_name(:casilla_cuatro) %>:</strong></dt>
  <dd><%= @bunker.casilla_cuatro %></dd>
  <dt><strong><%= model_class.human_attribute_name(:casilla_cinco) %>:</strong></dt>
  <dd><%= @bunker.casilla_cinco %></dd>
  <dt><strong><%= model_class.human_attribute_name(:casilla_seis) %>:</strong></dt>
  <dd><%= @bunker.casilla_seis %></dd>
  <dt><strong><%= model_class.human_attribute_name(:casilla_siete) %>:</strong></dt>
  <dd><%= @bunker.casilla_siete %></dd>
  <dt><strong><%= model_class.human_attribute_name(:casilla_ocho) %>:</strong></dt>
  <dd><%= @bunker.casilla_ocho %></dd>
  <dt><strong><%= model_class.human_attribute_name(:casilla_nueve) %>:</strong></dt>
  <dd><%= @bunker.casilla_nueve %></dd>
  <dt><strong><%= model_class.human_attribute_name(:casilla_diez) %>:</strong></dt>
  <dd><%= @bunker.casilla_diez %></dd>
  <dt><strong><%= model_class.human_attribute_name(:box_id) %>:</strong></dt>
  <dd><%= @bunker.box_id %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              bunkers_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_bunker_path(@bunker), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              bunker_path(@bunker),
              :method => 'delete',
              :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')),
              :class => 'btn btn-danger' %>
</div>
