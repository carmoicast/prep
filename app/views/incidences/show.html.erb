<%- model_class = @incidence.class -%>
<div class="page-header">
  <h1>Detalle incidencia</h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:hora) %>:</strong></dt>
  <dd><%= @incidence.hora %></dd>
  <dt><strong><%= model_class.human_attribute_name(:incidencia) %>:</strong></dt>
  <dd><%= @incidence.incidencia %></dd>
</dl>

<div class="form-actions">
  <%= link_to "Atras",
              incidences_path, :class => 'btn'  %>
  <%= link_to "Editar",
              edit_incidence_path(@incidence), :class => 'btn' %>
  <%= link_to "Elimnar",
              incidence_path(@incidence),
              :method => 'delete',
              :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'EstÃ¡ Seguro?')),
              :class => 'btn btn-danger' %>
</div>
