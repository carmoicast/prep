<%- model_class = @incidence.class -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:hora) %>:</strong></dt>
  <dd><%= @incidence.hora %></dd>
  <dt><strong><%= model_class.human_attribute_name(:tipo_incidente_apertura) %>:</strong></dt>
  <dd><%= @incidence.tipo_incidente_apertura %></dd>
  <dt><strong><%= model_class.human_attribute_name(:tipo_incidente_proceso) %>:</strong></dt>
  <dd><%= @incidence.tipo_incidente_proceso %></dd>
  <dt><strong><%= model_class.human_attribute_name(:tipo_incidente_cierre) %>:</strong></dt>
  <dd><%= @incidence.tipo_incidente_cierre %></dd>
  <dt><strong><%= model_class.human_attribute_name(:tipo_incidente_otro) %>:</strong></dt>
  <dd><%= @incidence.tipo_incidente_otro %></dd>
  <dt><strong><%= model_class.human_attribute_name(:box_id) %>:</strong></dt>
  <dd><%= @incidence.box_id %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              incidences_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_incidence_path(@incidence), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              incidence_path(@incidence),
              :method => 'delete',
              :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')),
              :class => 'btn btn-danger' %>
</div>
